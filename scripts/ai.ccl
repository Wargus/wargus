(define ai:air-attack-endloop
  '((writes nil "Looping !\n")
    (ai:force 1 (ai:flyer) 2)
    (ai:force 2 (ai:flyer) 2)
    (ai:force 3 (ai:flyer) 2)
    (ai:force 4 (ai:flyer) 2)
    (ai:force 5 (ai:flyer) 2)
    (ai:force 6 (ai:flyer) 1)
    (ai:wait-force 5)
