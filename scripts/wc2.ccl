;;=============================================================================
;;	Define race names
;;
;;	(define-race-names
;;	  'race '(
;;	    race number
;;	    name race-name
;;	    display displayed-race-name
;;	    [visible])
;;	  'race ...)

(define-race-names
  'race '(
    race 0
    name "human"
    display "Human"
    visible)
  'race '(
    race 1
    name "orc"
    display "Orc"
    visible)
  'race '(
    race 2
    name "neutral"
    display "Neutral"))

